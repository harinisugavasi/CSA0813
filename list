num1=[1,2,3,4]
num2=[5,6,7,8]
print(num1[3])
4
print(num2[3])
8
print(num1[1:3])
[2, 3]
print(num1[1:])
[2, 3, 4]
print(num1+num2)
[1, 2, 3, 4, 5, 6, 7, 8]
num2*3
[5, 6, 7, 8, 5, 6, 7, 8, 5, 6, 7, 8]
num1[2]=45
num1[2]=45
 num1.insert(3,"99")
  print(num1)
   [1, 2, 45, '99', 4]
num1.remove("99")
      
print(num1)
      
[1, 2, 45, 4]
